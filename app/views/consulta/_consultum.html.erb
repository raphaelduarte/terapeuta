<div id="<%= dom_id consultum %>">
  <p class="my-5">
    <strong class="block font-medium mb-1">Paciente:</strong>
    <%= consultum.paciente_id %>
  </p>

  <p class="my-5">
    <strong class="block font-medium mb-1">User:</strong>
    <%= consultum.user_id %>
  </p>

  <p class="my-5">
    <strong class="block font-medium mb-1">Data:</strong>
    <%= consultum.data_id %>
  </p>

  <% if action_name != "show" %>
    <%= link_to "Show this consultum", consultum, class: "rounded-lg py-3 px-5 bg-gray-100 inline-block font-medium" %>
    <%= link_to "Edit this consultum", edit_consultum_path(consultum), class: "rounded-lg py-3 ml-2 px-5 bg-gray-100 inline-block font-medium" %>
    <hr class="mt-6">
  <% end %>
</div>
