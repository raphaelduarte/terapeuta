<div class="mx-auto md:w-2/3 w-full flex">
  <div class="mx-auto">
    <% if notice.present? %>
      <p class="py-2 px-3 bg-green-50 mb-5 text-green-500 font-medium rounded-lg inline-block" id="notice"><%= notice %></p>
    <% end %>

    <%= render @historia_sexual_reprodutiva %>

    <%= link_to "Edit this historia_sexual_reprodutiva", edit_historia_sexual_reprodutiva_path(@historia_sexual_reprodutiva), class: "mt-2 rounded-lg py-3 px-5 bg-gray-100 inline-block font-medium" %>
    <div class="inline-block ml-2">
      <%= button_to "Destroy this historia_sexual_reprodutiva", historia_sexual_reprodutiva_path(@historia_sexual_reprodutiva), method: :delete, class: "mt-2 rounded-lg py-3 px-5 bg-gray-100 font-medium" %>
    </div>
    <%= link_to "Back to historia_sexual_reprodutivas", historia_sexual_reprodutivas_path, class: "ml-2 rounded-lg py-3 px-5 bg-gray-100 inline-block font-medium" %>
  </div>
</div>
