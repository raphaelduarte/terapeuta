<div id="<%= dom_id anamnese %>">
  <p class="my-5">
    <strong class="block font-medium mb-1">User:</strong>
    <%= anamnese.user_id %>
  </p>

  <p class="my-5">
    <strong class="block font-medium mb-1">Paciente:</strong>
    <%= anamnese.paciente_id %>
  </p>

  <% if action_name != "show" %>
    <%= link_to "Show this anamnese", anamnese, class: "rounded-lg py-3 px-5 bg-gray-100 inline-block font-medium" %>
    <%= link_to "Edit this anamnese", edit_anamnese_path(anamnese), class: "rounded-lg py-3 ml-2 px-5 bg-gray-100 inline-block font-medium" %>
    <hr class="mt-6">
  <% end %>
</div>
