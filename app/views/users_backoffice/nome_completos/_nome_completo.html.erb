<div id="<%= dom_id nome_completo %>">
  <% if action_name != "show" %>
    <%= link_to "Show this nome completo", nome_completo, class: "rounded-lg py-3 px-5 bg-gray-100 inline-block font-medium" %>
    <%= link_to "Edit this nome completo", edit_nome_completo_path(nome_completo), class: "rounded-lg py-3 ml-2 px-5 bg-gray-100 inline-block font-medium" %>
    <hr class="mt-6">
  <% end %>
</div>
